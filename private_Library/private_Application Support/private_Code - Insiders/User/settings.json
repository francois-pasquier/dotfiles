{
    "workbench.colorTheme": "Catppuccin Macchiato",
    "editor.fontFamily": "CartographCF Nerd Font",
    "editor.codeLensFontFamily": "CartographCF Nerd Font",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.tabSize": 2,
    "editor.inlayHints.fontFamily": "CartographCF Nerd Font",
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "extensions.trustedPublishers": [
        "bradlc",
        "catppuccin",
        "eamodio",
        "esbenp",
        "mathiasfrohlich",
        "mrorz",
        "naumovs",
        "octref",
        "shd101wyy",
        "skellock",
        "swmansion",
        "vscodevim",
        "wayou",
        "webossdk",
        "yzhang"
    ],
    "chat.tools.terminal.outputLocation": "chat",
    "nrf-connect.sdkManager.region": "default",
    "nrf-connect.enableTelemetry": false,
    "claudeCode.preferredLocation": "panel"
}